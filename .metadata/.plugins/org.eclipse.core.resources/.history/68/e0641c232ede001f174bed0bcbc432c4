package smepractice.java;

class B 
{
    String name;
    int age;

    B(String s, int a)
    {
        name = s;
        age = a;
    }

    void show() {
        System.out.println("Your name is " + name + " and your age is " + age);
    }
}

public class Constructor {
    public static void main(String[] args) {
        B obj = new B("suguna", 24);
        obj.show();
    }
}
